{
  "curly": true,    // Warn no curly brancket in loop or conditions
  "undef": true,    // Warn unused variable (that will become global variable)
  "unused": "vars", // Warn unused variables, not detect arguments
  "newcap": true,   // Warn if first letter is not capital
  "nonew": true,    // Warn contructor call
  "regexp": true,   // Warn use . in regexp
  "eqeqeq": true,   // Warn ==, != to compare
  "latedef": true,  // Warn use variable before definition
  "esnext": true,   // Enable ES6 features

  // false means enable access, prohibits overwrite
  "globals": {
    "pzpr": false
  },
  "browser": true,  // Allow to use browser defined variable (not console, alert etc.)
  "node": true,     // Allow to use node defined variable (require etc.)

  "shadow": true,   // Allow redefine variable (TODO: this option disable)
  "loopfunc": true, // Allow function definition in loop
  "sub": true       // Allow object access via obj["name"]
}
